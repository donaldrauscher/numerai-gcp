{
  "dataset_params": {
    "version": "v4.3",
    "feature_set": "small"
  },
  "model_params": {
    "n_estimators": 200,
    "learning_rate": 0.1,
    "max_depth": 5,
    "num_leaves": 32,
    "colsample_bytree": 0.1
  },
  "training_params": {
    "eras_per_training_block": 104
  },
  "ensemble_params": {
    "targets": [
      "target_cyrus_v4_20",
      "target_teager_v4_20"
    ],
    "alpha": 0.05,
    "fit_intercept": false,
    "positive": true
  },
  "neutralize_params": {
    "strategy": "all_features",
    "proportion": 0.25
  }
}
